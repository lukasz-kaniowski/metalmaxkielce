{
  "metadata": {
    "title": "metalmaxkielce.pl",
    "description": "Stron Metal Max SC"
  },
  "plugins": {
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-in-place": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-watch": {
      "paths": {
        "${source}/**/*": true,
        "partials/**/*": "**/*.html",
        "layouts/**/*": "**/*.html"
      },
      "livereload": true
    }
  }
}
